!TerniLapilli methodsFor: 'as yet unclassified' stamp: 'DS 11/1/2020 13:02:04' prior: 50912463!
moveOn: aDirection from: aPosition

	| occupiedPosition |

	((self occupiedPositionsByCurrentPlayer ) size = 3 ) 		ifFalse: [ self error: self class remainedTokensErrorDescription ].
	(board includes: {currentPlayer. aPosition } ) ifFalse: [ self error: self class ilegalMovementErrorDescription ].	
	
	
	(aPosition + aDirection between: (-1@-1) and: (1@1)) ifFalse: [ self error: self class positionOutOfBoardErrorDescription ].
	(self isOver) ifTrue: [ self error: self class gameOverErrorDescription ].
	((board collect: [ :aOccupiedPosition | aOccupiedPosition second] ) includes: aPosition + aDirection) 		
		ifTrue: [ self error: self class ilegalMovementErrorDescription ].


	occupiedPosition := {currentPlayer. aPosition}.
	self add: aPosition + aDirection.
	board remove: occupiedPosition.

	! !
