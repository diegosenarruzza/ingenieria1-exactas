	((board collect: [ :aOccupiedPosition | aOccupiedPosition second] ) includes: aPosition + aDirection) 		
		ifTrue: [ self error: self class ilegalMovementErrorDescription ].


	self addMoveOn: aDirection from: aPosition.

	! !
