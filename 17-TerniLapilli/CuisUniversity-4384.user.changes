!TerniLapilli methodsFor: 'movements' stamp: 'DS 11/1/2020 01:14:00' prior: 50907577!
moveOn: aDirection from: aPosition

	| occupiedPosition |

	(positionsOccupied includes: {currentPlayer. aPosition } ) ifFalse: [ self error: self class ilegalMovementErrorDescription ].	

	occupiedPosition := {currentPlayer. aPosition}.

	self put: currentPlayer at: aPosition + aDirection .
	positionsOccupied remove: occupiedPosition.

	! !

