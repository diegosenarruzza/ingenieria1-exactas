moveOn: aDirection from: aPosition

	| occupiedPosition |

	((self occupiedPositionsByCurrentPlayer ) size = 3 ) 		ifFalse: [ self error: self class remainedTokensErrorDescription ].
	(board includes: {currentPlayer. aPosition } ) ifFalse: [ self error: self class ilegalMovementErrorDescription ].	
	
	
	(aPosition + aDirection between: (-1@-1) and: (1@1)) ifFalse: [ self error: self class positionOutOfBoardErrorDescription ].
