moveLeftUpXFrom: aPosition

	| direction |

	(0@-1 = aPosition) ifTrue: [ self error: self class ilegalMovementErrorDescription ].

	direction := -1@1.

	self put: #X at: aPosition + direction.
	positionsOccupied remove: {#X. aPosition}.

	! !

